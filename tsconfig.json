{
	"extends": "@tsconfig/svelte/tsconfig.json",
	"include": ["src/**/*", "src/node_modules/**/*"],
	"exclude": ["node_modules/*", "__sapper__/*", "static/*"],
	"compilerOptions": {
		"lib": ["DOM", "ES2017", "WebWorker"],
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"importsNotUsedAsValues": "error",
		"strict": true,
		"baseUrl": "./",
		"paths": { "@/*": ["src/*"] }
	}
}